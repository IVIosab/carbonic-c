routine fib (x : integer) : integer is 
    while (x /= 1) or (x /= 0) loop
        var y : integer is fib(x-1);
        var z : integer is fib(x-2);
        var res : integer is y+z;
        return res;
    end
    return 1;
end

routine main () : integer is 
    var input : integer is 10;
    print(fib(input));
    return 0;
end
